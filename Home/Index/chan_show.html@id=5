<script type="text/javascript">

var wapkuan=document.body.clientWidth;

    var gaos=$(".nav-height").offset().top;

    var t01=$("#Tony_1").offset().top;
    var t02=$("#Tony_2").offset().top;
    var t03=$("#Tony_3").offset().top;
    var t04=$("#Tony_4").offset().top;


if(wapkuan<768){

	setInterval(function(){

		var rc=$(document).scrollTop();
		var dwgao=rc+1;

		if(dwgao>t01){
			$(".dingw01 a").addClass("dq"); 
		}else{
			$(".dingw01 a").removeClass("dq"); 	
		}
		
		if(dwgao>t02){
			$(".dingw01 a").removeClass("dq"); 
			$(".dingw02 a").addClass("dq"); 
		}else{
			$(".dingw02 a").removeClass("dq"); 	
		}
		
		if(dwgao>t03){
			$(".dingw01 a").removeClass("dq"); 
			$(".dingw02 a").removeClass("dq"); 
			$(".dingw03 a").addClass("dq"); 
		}else{
			$(".dingw03 a").removeClass("dq"); 	
		}
		
		if(dwgao>t04){
			$(".dingw01 a").removeClass("dq"); 
			$(".dingw02 a").removeClass("dq"); 
			$(".dingw03 a").removeClass("dq"); 
			$(".dingw04 a").addClass("dq"); 
		}else{
			$(".dingw04 a").removeClass("dq"); 	
		}

		if(rc > gaos){
            $(".go-top").show();
 		}else{
            $(".go-top").hide();
		}	
		
	},10)
}else{



	setInterval(function(){

		var rc=$(document).scrollTop();
		var dwgao=rc+1;
		if(dwgao>t01){
			$(".dingw01 a").addClass("dq"); 
		}else{
			$(".dingw01 a").removeClass("dq"); 	
		}
		
		if(dwgao>t02){
			$(".dingw01 a").removeClass("dq"); 
			$(".dingw02 a").addClass("dq"); 
		}else{
			$(".dingw02 a").removeClass("dq"); 	
		}
		
		if(dwgao>t03){
			$(".dingw01 a").removeClass("dq"); 
			$(".dingw02 a").removeClass("dq"); 
			$(".dingw03 a").addClass("dq"); 
		}else{
			$(".dingw03 a").removeClass("dq"); 	
		}
		
		if(dwgao>t04){
			$(".dingw01 a").removeClass("dq"); 
			$(".dingw02 a").removeClass("dq"); 
			$(".dingw03 a").removeClass("dq"); 
			$(".dingw04 a").addClass("dq"); 
		}else{
			$(".dingw04 a").removeClass("dq"); 	
		}

		if(rc > gaos){
            $(".go-top").show();
 		}else{
            $(".go-top").hide();
		}	


	},10)



	}


  var ar = $("#arr").html();

  if(ar == 'ARRAY' || ar == 'Array'){

    $("#arr").html('No parameter');

  }
</script>

