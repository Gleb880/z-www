<script type="text/javascript">

    function part7(){
        var p7Width=document.querySelector(".part7_rb").getBoundingClientRect().width;
        var part7_xian = document.getElementById("part7_xian");
        part7_xian.style.width = p7Width+"px";
    }
    part7();

    function part8(){
        var p7Width=document.querySelector(".part7_rb").getBoundingClientRect().width;
        var part7_xian = document.getElementById("part7_xian");
        part7_xian.style.width = p7Width+"px";
    };
    part7();

    $(document).ready(function () {
        isScroll.init('.zitest');
        $(window).resize(function () {
            part7();
        });
    });




</script>

<script type="text/javascript">
var wapkuan=document.body.clientWidth;
var gaos=$(".nav-height").offset().top;

if(wapkuan<768){
    setInterval(function(){
        var rc=$(document).scrollTop();
        if(rc > gaos){
            $(".go-top").show();
        }else{
            $(".go-top").hide();
        }
    },10)
}else{
    setInterval(function(){
        var rc=$(document).scrollTop();
        if(rc > gaos){
            $(".go-top").show();
        }else{
            $(".go-top").hide();
        }   
    },10)
    }
</script>
