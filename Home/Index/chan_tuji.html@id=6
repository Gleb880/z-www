<script type="text/javascript">
var wapkuan=document.body.clientWidth;
var gaos=$(".nav-height").offset().top;

if(wapkuan<768){
	setInterval(function(){
		var rc=$(document).scrollTop();
		if(rc > gaos){
            $(".go-top").show();
 		}else{
            $(".go-top").hide();
		}
	},10)
}else{
	setInterval(function(){
		var rc=$(document).scrollTop();
		if(rc > gaos){
            $(".go-top").show();
 		}else{
            $(".go-top").hide();
		}	
	},10)
	}
</script>
